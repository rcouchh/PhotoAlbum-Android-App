<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.photoalbum"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.photoalbum.Main_menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.photoalbum.CreateAlbum"
            android:label="@string/title_activity_create_album" >
        </activity>
        <activity
            android:name="com.example.photoalbum.OpenedAlbum"
            android:label="@string/title_activity_opened_album" >
        </activity>
        <activity
            android:name="com.example.photoalbum.DisplayPhoto"
            android:label="@string/title_activity_display_photo" >
        </activity>
        <activity
            android:name="com.example.photoalbum.RenameAlbum"
            android:label="@string/title_activity_rename_album" >
        </activity>
        <activity
            android:name="com.example.photoalbum.SearchPhotos"
            android:label="@string/title_activity_search_photos" >
        </activity>
    </application>

</manifest>
